{
  "name": "forge-exchange",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm -r run dev",
    "build": "pnpm -r build",
    "start": "pnpm -r run start",
    "check": "pnpm -r run check",
    "db:push": "pnpm -r run db:push",
    "lint": "pnpm -r run lint",
    "install:services": "pnpm install --workspace",
    "start:services": "pnpm --filter relayer start",
    "start:gateway": "pnpm --filter gateway dev",
    "start:price-engine": "node apps/price-engine/index.js",
    "test": "vitest",
    "test:relayer": "npx anvil --fork-url https://mainnet.base.org --fork-block-number 40500000 & npx wait-on tcp:8545 && npx dotenv -e apps/relayer/testing.env -- vitest run --config apps/relayer/vite.config.ts apps/relayer/test/integration"
  },
  "overrides": {
    "drizzle-kit": "latest",
    "esbuild": "latest",
    "drizzle-orm": "0.38.4",
    "drizzle-zod": "0.6.0",
    "qs": "6.14.1",
    "@solana-program/system": "0.1.3",
    "@solana/web3.js": "1.95.0"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.2",
    "typescript": "^5.9.3",
    "viem": "^2.43.5",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0",
    "@types/pg": "^8.16.0",
    "pg": "^8.16.3",
    "sonner": "^1.4.3",
    "wagmi": "^2.5.7"
  }
}
