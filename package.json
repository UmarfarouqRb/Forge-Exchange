{
  "name": "forge-exchange",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "npx turbo run dev --",
    "build": "turbo run build",
    "start": "turbo run start",
    "check": "turbo run check",
    "db:push": "turbo run db:push",
    "lint": "turbo run lint",
    "install:services": "pnpm install --workspace",
    "start:services": "turbo run start --filter='./apps/relayer'",
    "start:price-engine": "node apps/price-engine/index.js",
    "test": "vitest",
    "test:relayer": "dotenv -e apps/relayer/testing.env -- vitest run apps/relayer/test/integration"
  },
  "overrides": {
    "drizzle-kit": "latest",
    "esbuild": "latest",
    "drizzle-orm": "0.38.4",
    "drizzle-zod": "0.6.0",
    "qs": "6.14.1",
    "@solana-program/system": "0.1.3",
    "@solana/web3.js": "1.95.0"
  },
  "devDependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "dotenv-cli": "^7.4.2",
    "turbo": "^2.7.3",
    "viem": "^2.43.5",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@9.15.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.88.0",
    "@types/pg": "^8.16.0",
    "pg": "^8.16.3"
  }
}